{
  "name": "app",
  "version": "2.7.8",
  "private": true,
  "scripts": {
    "bump": "bump patch --tag --commit 'testnet release '",
    "bump:minor": "bump minor --tag --commit 'testnet release '",
    "dev": "vite --host --port 8080",
    "build": "vite build",
    "test": "vitest run"
  },
  "dependencies": {
    "@sifchain/sdk": "workspace:*",
    "@sifchain/wallet-keplr": "workspace:*",
    "@sifchain/wallet-metamask": "workspace:*",
    "@sifchain/wallet-terra-station": "workspace:*",
    "bignumber.js": "^9.0.2",
    "buffer": "^6.0.3",
    "clsx": "^1.1.1",
    "copy-to-clipboard": "^3.3.1",
    "core-js": "^3.22.7",
    "dat.gui": "^0.7.9",
    "date-fns": "^2.28.0",
    "immer": "^9.0.14",
    "p-limit": "^4.0.0",
    "p-props": "^5.0.0",
    "react-dom": "^17.0.2",
    "rxjs": "7.5.5",
    "tippy.js": "^6.3.7",
    "vue": "^3.2.37",
    "vue-query": "^1.25.2",
    "vue-router": "^4.1.3",
    "vue-tippy": "^6.0.0-alpha.57",
    "vuex": "4.0.2"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^16.2.1",
    "@jsdevtools/version-bump-prompt": "^6.1.0",
    "@tailwindcss/typography": "^0.5.2",
    "@types/dat.gui": "^0.7.7",
    "@typescript-eslint/eslint-plugin": "^5.26.0",
    "@typescript-eslint/parser": "^5.26.0",
    "@vitejs/plugin-vue": "^3.0.1",
    "@vitejs/plugin-vue-jsx": "^2.0.0",
    "@vue/compiler-sfc": "^3.2.37",
    "@vue/eslint-config-typescript": "^11.0.0",
    "@vue/test-utils": "^2.0.2",
    "@vue/tsconfig": "^0.1.3",
    "autoprefixer": "^10.4.8",
    "eslint": "^8.21.0",
    "eslint-plugin-vue": "^9.3.0",
    "happy-dom": "^6.0.4",
    "postcss": "^8.4.14",
    "prettier": "^2.6.2",
    "prettier-plugin-tailwindcss": "^0.1.13",
    "rollup-plugin-analyzer": "^4.0.0",
    "rollup-plugin-visualizer": "^5.7.1",
    "sass": "^1.50.1",
    "style-loader": "^3.3.1",
    "svgo": "^2.8.0",
    "tailwindcss": "^3.1.4",
    "typescript": "^4.7.4",
    "vite": "^3.0.4",
    "vite-compatible-readable-stream": "^3.6.1",
    "vite-plugin-html": "^3.2.0",
    "vite-plugin-live-reload": "^3.0.0",
    "vite-plugin-singlefile": "^0.11.0",
    "vite-svg-loader": "^3.4.0",
    "vitest": "^0.20.3"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/vue3-essential",
      "eslint:recommended",
      "@vue/typescript/recommended"
    ],
    "parserOptions": {
      "ecmaVersion": 2020
    },
    "rules": {
      "@typescript-eslint/no-unused-vars": "off",
      "@typescript-eslint/no-explicit-any": "off",
      "@typescript-eslint/no-empty-function": "off",
      "@typescript-eslint/explicit-module-boundary-types": "off"
    },
    "overrides": [
      {
        "files": [
          "**/__tests__/*.{j,t}s?(x)",
          "**/tests/unit/**/*.spec.{j,t}s?(x)",
          "**/*.test.{j,t}s?(x)"
        ]
      }
    ]
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ],
  "jest": {
    "testMatch": [
      "**/**.test.ts",
      "**/tests/unit/*.js"
    ],
    "preset": "@vue/cli-plugin-unit-jest/presets/typescript-and-babel",
    "transform": {
      "^.+\\.vue$": "vue-jest"
    }
  }
}
